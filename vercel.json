{
  "version": 2,
  "builds": [
    {
      "src": "front-end/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "front-end/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "front-end/dist/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "front-end/dist/index.html"
    }
  ]
} 